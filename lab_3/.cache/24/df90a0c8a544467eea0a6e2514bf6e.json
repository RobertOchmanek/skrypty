   update() {\r\n        if (this.clicked) {\r\n            this.clicked = false;\r\n            setTimeout(() => {\r\n                this.scene.start('Game');\r\n            }, 250);\r\n        }\r\n    }\r\n}\r\n\r\nexport default StartScreen;\r\n"},"lineCount":null}},"error":null,"hash":"cf3c13294dac776555b917752521dd11","cacheData":{"env":{}}}