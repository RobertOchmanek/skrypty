ed":{"line":117,"column":15},"source":"src/gameObjects/Coin.js","original":{"line":53,"column":15}}],"sources":{"src/gameObjects/Coin.js":"import increaseScore from \"../ui/increaseScore\";\r\n\r\nclass Coin {\r\n\r\n    constructor(scene) {\r\n        this.scene = scene;\r\n\r\n        this.coins = this.scene.physics.add.group({\r\n            immovable: true,\r\n            allowGravity: false\r\n        });\r\n\r\n        const coinObjects = this.scene.map.getObjectLayer('coins').objects;\r\n\r\n        for (const coin of coinObjects) {\r\n            this.coins.create(coin.x, coin.y, 'atlas')\r\n                .setOrigin(0)\r\n                .setDepth(-1);\r\n        }\r\n    }\r\n\r\n    collideWith(gameObject) {\r\n        this.scene.physics.add.overlap(this.coins, gameObject, this.collect, null, this);\r\n        return this;\r\n    }\r\n\r\n    update() {\r\n        for (const coin of this.coins.children.entries) {\r\n            coin.play('rotate', true);\r\n        }\r\n    }\r\n\r\n    collect() {\r\n        for (const coin of this.coins.children.entries) {\r\n            if (!coin.body.touching.none) {\r\n                coin.body.setEnable(false);\r\n\r\n                this.scene.tweens.add({\r\n                    targets: coin,\r\n                    ease: 'Power1',\r\n                    scaleX: 0,\r\n                    scaleY: 0,\r\n                    duration: 200,\r\n                    onComplete: () => coin.destroy()\r\n                });\r\n            }\r\n        }\r\n\r\n        increaseScore(1);\r\n    }\r\n}\r\n\r\nexport default Coin;\r\n"},"lineCount":null}},"error":null,"hash":"de2aee45da491b9fd68d58b98e065180","cacheData":{"env":{}}}